<table id="example1" class="table table-bordered">
    <thead>
    <tr>
      <th>No</th>
      <th>Nama</th>
      <th>Jenis</th>
      <th>Kota</th>
      <th>Action</th>
    </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i<rumahSakit.length; i++){ %>
            <tr>
                <td><%= i+1 %></td>
                <td><%= rumahSakit[i].nama %></td>
                <td><%= rumahSakit[i].jenis %></td>
                <td><%= rumahSakit[i].kabupaten %></td>
                <td>
                    <a href="/rumahsakit/edit/<%= rumahSakit[i]._id %>" class="btn btn-info btn-sm">
                        <i class="fas fa-pen mr-1"></i> Edit
                    </a>
                </td>
            </tr>
        <% } %>
    </tbody>
  </table>